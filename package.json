    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "firebase": "^9.8.2",
    "firebase-admin": "^10.2.0",
    "googleapis": "^100.0.0",
    "marked": "^4.0.15",
    "quill-autoformat": "^0.1.2",
    "quill-image-uploader": "^1.2.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-draft-wysiwyg": "^1.14.7",
    "react-google-drive-picker": "1.1.4",
    "react-modal": "^3.15.1",
    "react-quill": "^1.3.5",
    "remix": "^1.5.1",
